{
  "name": "mern-starter",
  "version": "1.0.0",
  "description": "starter for mern with passport auth",
  "main": "server.ts",
  "engines": {
    "node": "16.13.2"
  },
  "scripts": {
    "dev": "concurrently \"nodemon source/server.ts\" \"cd .. && cd client && npm start\"",
    "deploy": "git add . && git commit -m Heroku && git push heroku master",
    "postinstall": "tsc"
  },
  "keywords": [
    "typescript",
    "express",
    "mongoose",
    "passport",
    "oauth",
    "react",
    "mern"
  ],
  "author": "Joanthan Potter",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cartoon-avatar": "^1.0.2",
    "connect-mongo": "^4.6.0",
    "cors": "^2.8.5",
    "dotenv": "^14.3.2",
    "express": "^4.17.2",
    "express-flash": "^0.0.2",
    "express-session": "^1.17.2",
    "mongoose": "^6.1.8",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.15",
    "passport": "^0.5.2",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/connect-mongo": "^3.1.3",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/express-flash": "^0.0.2",
    "@types/express-session": "^1.17.4",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.12",
    "@types/passport": "^1.0.7",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-local": "^1.0.34",
    "concurrently": "^7.0.0"
  }
}
